    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start Server",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/manage.py"
        },
        {
          "name": "Make Migrations",
          "type": "debugpy",
          "request": "launch",
          "args": [
            "makemigrations"
          ],
          "django": true,
          "program": "${workspaceFolder}/manage.py",
        },
        {
          "name": "Migrate",
          "type": "debugpy",
          "request": "launch",
          "args": [
            "migrate"
          ],
          "django": true,
          "program": "${workspaceFolder}/manage.py",
        },
        {
            "name": "Seed Language",
            "type": "debugpy",
            "request": "launch",
            "args": [
              "seed_language"
            ],
            "django": true,
            "program": "${workspaceFolder}/manage.py",
          },
    ]
}